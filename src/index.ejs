  <!-- Dynamic Title Tag for Plugins -->
  <script>
    let getTitleOfPage = function () {
      let getURL = location.pathname;
      let appendZuri = `- Zuri Chat `;

      if (getURL !== '/') {
        getURL = getURL.substr(1);
        getURL = getURL.charAt(0).toUpperCase() + getURL.slice(1);
        document.title = getURL + appendZuri;
      };
    };
    setInterval(getTitleOfPage, 500)
  </script>

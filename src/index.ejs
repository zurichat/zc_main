  <style>
    html,
    body {
      height: 100%;
      width: 100%;
      margin: 0;
      padding: 0;
      /* font-size: 100% !important; */
    }

    div[id*='zuri'] {
      width: 100%;
      overflow: hidden;
    }

    div[id*='zuri/topbar'] {
      z-index: 5;
      width: 100%;
      height: 6vh;
      position: fixed;
      top: 0;
      left: 0;
      /* text-align: center; */
      display: flex;
      /* justify-content: center; */
      align-items: center;
      background-color: #fff;
      z-index: 999999;
      overflow: unset;
    }

    div[id*='zuri/sidebar']::-webkit-scrollbar {
      display: none;
    }

    div[id*='zuri/sidebar'] {
      width: 20%;
      /* margin-right: 2%; */
      height: 94vh;
      /* overflow-y: scroll; */
      /* Hide scrollbar for IE and Edge */
      -ms-overflow-style: none;
      /* Hide scrollbar for Firefox */
      scrollbar-width: none;
      position: fixed;
      top: 6vh;
      left: 0;
      background-color: #fff;
      z-index: 99999;

    }

    div[id*='zuri/commentSidebar'] {
      width: 299px;
      position: absolute;
      background-color: #e5e5e5;
      right: 0;
      top: 20vh;
      z-index: 9999;
    }

    div[id*='zuri-plugin'] {
      width: 80%;
      position: absolute;
      top: 6vh;
      right: 0;
      z-index: unset;
    }

    @media (min-width: 1441px) {
      div[id*='zuri/topbar'] {
        height: 5vh;
        top: 0;
        left: 0;
      }

      div[id*='zuri/sidebar']::-webkit-scrollbar {
        display: none;
      }

      div[id*='zuri/sidebar'] {
        height: 95vh;
        top: 5vh;
        left: 0;
      }

      div[id*='zuri-plugin'] {
        top: 5vh;
        right: 0;
        z-index: unset;
      }
    }

    @media (max-width: 768px) {
      div[id*='zuri/sidebar'] {
        display: none;
        width: 100%;
      }

      div[id*='zuri-plugin'] {
        width: 100%;
        z-index: unset;
      }
    }

    /* CHESS STYLE */
    .app__container {
      padding: 0 !important;
      padding-block: 5rem;
    }

    @media (max-width: 425px) {
      /* div[id*='zuri/sidebar'] {
        width: 40%;
      } */

      div[id*='zuri-plugin'] {
        width: 100%;
        position: relative;
      }
    }

    @media (max-width: 375px) {
      .mini-board {
        height: calc(34.8rem * 0.9);
        width: calc(26.4rem * 0.9);
      }
    }




